{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "client/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "client/static/$1"
    },
    {
      "src": "/(.\\.(js|css|png|jpg|jpeg|svg|ico|json)$)",
      "dest": "client/$1"
    },
    {
      "src": "/(.*)",
      "dest": "client/index.html"
    }
  ]
}
